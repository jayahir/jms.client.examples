  <groupId>org.activemq.jms</groupId>
  <artifactId>jms.examples.parent</artifactId>
  <version>0.0.1-SNAPSHOT</version>
  <packaging>pom</packaging>

  <name>JMS Examples :: Parent</name>

  <url>https://github.com/joelicious/jms.client.examples/</url>

  <scm>
    <url>https://github.com/joelicious/jms.client.examples.git</url>
    <connection>scm:git:git://github.com/joelicious/jms.client.examples.git</connection>
    <developerConnection>scm:git:git@github.com:joelicious/jms.client.examples.git</developerConnection>
  </scm>

  <developers>
    <developer>
      <id>jbutler</id>
      <name>Joseph Butler</name>
      <email>jbutler@redhat.com</email>
      <organization>Red Hat</organization>
      <organizationUrl>http://redhat.com</organizationUrl>
      <roles>
        <role>Consultant</role>
      </roles>
    </developer>
  </developers>

  <modules>
    <module>activemq.camel.connectionpooling.client</module>
    <module>artemis.camel.cluster.producer</module>
    <module>artemis.camel.cluster.consumer</module>
    <module>artemis.camel.connectionpooling.producer</module>
