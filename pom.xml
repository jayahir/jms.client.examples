      <id>jboss.releases</id>
      <name>JBoss Fuse Release Repository</name>
      <url>https://repository.jboss.org/nexus/content/repositories/fs-releases/</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </repository>
    <repository>
      <id>jboss.ea</id>
      <name>JBoss Fuse Early Access Release Repository</name>
      <url>https://repository.jboss.org/nexus/content/groups/ea/</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <releases>
        <enabled>true</enabled>
      </releases>
    </repository>
  </repositories>
  <pluginRepositories>
    <pluginRepository>
      <id>jboss.releases</id>
      <name>JBoss Fuse Release Repository</name>
      <url>https://repository.jboss.org/nexus/content/repositories/fs-releases/</url>
      <releases>
        <enabled>true</enabled>
      </releases>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
    </pluginRepository>
    <pluginRepository>
      <id>jboss.ea</id>
      <name>JBoss Fuse Early Access Release Repository</name>
      <url>https://repository.jboss.org/nexus/content/groups/ea/</url>
      <snapshots>
        <enabled>false</enabled>
      </snapshots>
      <releases>
        <enabled>true</enabled>
      </releases>
    </pluginRepository>
